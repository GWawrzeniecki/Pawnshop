<UserControl x:Class="PawnShop.CoreViews.Views.LogoutIcon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/" xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
             prism:ViewModelLocator.AutoWireViewModel="True">



    <Button  Background="{x:Null}" BorderBrush="{x:Null}" Command="{Binding LogoutCommand}">
        <iconPacks:PackIconMaterial Kind="Power"  />
    </Button>


</UserControl>
